package starWarsCharacters;

public class Character
{
private String name;
private String race;
private String description;
public String getName()
{
return name;
}
public void setName
(String name
)
{
this.name = name;
}
public String getRace()
{
return race;
}
public void setRace
(String race
)
{
this.race = race;
}
public String getDescription()
{
return description;
}
public void setDescription
(String description
)
{
this.description = description;
}
}
